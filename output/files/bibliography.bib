@article{Collatz,
    author = {Carolin Z\"obelein},
    title = {About the proof of the {C}ollatz conjecture},
    year = {2013},
    Eprint = {arXiv:1303.2073},
    url = {https://arxiv.org/abs/1303.2073},
    note = {Preprint, \url{https://arxiv.org/abs/1303.2073}, LaTeX: \url{https://github.com/Samdney/paper-collatz}},
    abstract = {I want to show one possibility to proof the Collatz conjecture, also called $3n + 1$ conjecture, for any natural number $N \in \mathbb{N}$. For this, I limit my analysis on the direct odd follower of every natural odd number and show the connections between the already by one reached numbers and their followers, to find an recurrence over all ranges $\left[1, N_{i}\right]$, $i \in \mathbb{N}$ to proof the conjecture.}
}
